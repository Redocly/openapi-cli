{
    "enableCodeframe": true,
    "enbaleCustomRuleset": true,
    "rules": {
        "api-servers": "off",
        "no-extra-fields": {
            "level": "warning"
        },
        "path-param-exists": {
            "level": "error"
        },
        "noUnusedComponents": "off",
        "licenseRequired": "on",
        "oas3-schema": {
            "level": "warning",

            "schema": {
                "level": "error"
            },
            "license": {
                "level": "error"
            }
        }
    }
}